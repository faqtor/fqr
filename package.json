{
  "name": "fqr",
  "version": "0.0.1",
  "description": "Faqtor build automation tool",
  "repository": {
    "type": "git",
    "url": "https://github.com/faqtor/fqr"
  },
  "bin": {
    "fqr": "bin/fqr.js"
  },
  "license": "MIT",
  "keywords": [
    "build",
    "make",
    "task",
    "build automation"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "bin"
  ],
  "scripts": {
    "build:es": "rollup -c build/rollup.config.js -i src/index.ts -o dist/index.es.js -m -f es -n fqr",
    "build:cjs": "rollup -c build/rollup.config.js -i src/index.ts -o dist/index.cjs.js -m -f cjs -n fqr",
    "build": "npm run build:cjs && npm run build:es",
    "clean:dist": "rimraf ./dist",
    "clean:all": "rimraf ./dist ./node_modules ./.rpt2_cache"
  },
  "author": "Osman Bineev",
  "devDependencies": {
    "rimraf": "^2.6.2",
    "rollup": "^0.67.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-typescript2": "^0.17.2",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "@types/glob": "^7.1.1",
    "@types/node": "^10.12.18",
    "@types/string-argv": "^0.1.0",
    "@types/which": "^1.3.1",
    "glob": "^7.1.3",
    "resolve-bin": "^0.4.0",
    "string-argv": "^0.1.1",
    "which": "^1.3.1"
  }
}
